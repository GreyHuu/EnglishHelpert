<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ReadingQuestionEntity" table="reading_question" schema="eng">
        <id name="id" column="id"/>
        <property name="readingQuestion" column="reading_question"/>
        <property name="readingChoose" column="reading_choose"/>
        <property name="readingId" column="reading_id"/>
    </class>
</hibernate-mapping>