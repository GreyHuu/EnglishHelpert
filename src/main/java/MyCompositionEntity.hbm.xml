<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.MyCompositionEntity" table="myComposition" schema="eng">
        <id name="mycptId" column="mycpt_id"/>
        <property name="cptId" column="cpt_id"/>
        <property name="mycpt" column="mycpt"/>
        <property name="mycptCreateTime" column="mycpt_create_time"/>
        <property name="mycptWordCount" column="mycpt_word_count"/>
        <property name="userId" column="user_id"/>
        <property name="cptTitle" column="cpt_title"/>
    </class>
</hibernate-mapping>